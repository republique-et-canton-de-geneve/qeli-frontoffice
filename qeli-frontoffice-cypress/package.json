{
  "name": "qeli-frontoffice-cypress",
  "version": "0.0.1",
  "description": "Cypress avec Cucumber pour tester l'application web",
  "scripts": {
    "cy:verify": "cypress verify",
    "cy:version": "cypress version",
    "cy:open:ci": "QELI_ENV=ci npm run cy:open",
    "cy:open:prod": "QELI_ENV=prod npm run cy:open",
    "cy:open": "cypress open",
    "cy:run:ci": "QELI_ENV=ci npm run test",
    "cy:run:prod": "QELI_ENV=prod npm run test",
    "test": "cypress run"
  },
  "private": true,
  "dependencies": {
    "cypress": "~3.8.2",
    "cypress-cucumber-preprocessor": "~2.0.1"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  }
}
