{
  "name": "qeli-frontoffice-cypress",
  "version": "0.0.1",
  "description": "Cypress avec Cucumber pour tester l'application web",
  "scripts": {
    "cy:run" : "cypress run",
    "cy:run:ci" : "cypress run --env configFile=ci",
    "cy:verify": "cypress verify",
    "cy:version": "cypress version",
    "start:ci": "cd ../qeli-frontoffice-application && mvn spring-boot:run -Pdevelopment"
  },
  "private": true,
  "dependencies": {
    "cypress": "~3.8.2",
    "cypress-cucumber-preprocessor": "~2.0.1"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  }
}
