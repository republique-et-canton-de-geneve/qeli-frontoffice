<botdetect xmlns="https://captcha.com/schema/java"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="https://captcha.com/schema/java
                               https://captcha.com/schema/java/botdetect-4.0.beta3.7.xsd">

    <captchaStyles>
        <captchaStyle>
            <name>qeliCaptchaStyle</name>
            <userInputID>qeliCaptchaUserInput</userInputID>
            <imageTooltip>Texte à saisir</imageTooltip>
            <soundTooltip>Obtenir une version audio</soundTooltip>
            <reloadTooltip>Générer un nouveau texte</reloadTooltip>
            <helpLinkText>Custom Captcha help link text</helpLinkText>
            <helpLinkEnabled>false</helpLinkEnabled>
        </captchaStyle>
    </captchaStyles>
</botdetect>
